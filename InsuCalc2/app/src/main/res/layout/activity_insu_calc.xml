<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C7E1FF"
    tools:context=".InsuCalc"
    android:clickable="true"
    android:focusableInTouchMode="true">

    <View
        android:id="@+id/myRectangleView5"
        android:layout_width="290dp"
        android:layout_height="230dp"
        android:layout_marginStart="304dp"
        android:layout_marginBottom="248dp"
        android:background="@drawable/rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <com.jjoe64.graphview.GraphView
        android:id="@+id/graph"
        android:layout_width="586dp"
        android:layout_height="286dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/rectangle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:seriesColor="#000000" />

    <View
        android:id="@+id/myRectangleView"
        android:layout_width="288dp"
        android:layout_height="444dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/graph"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/simulate"
        android:layout_width="290dp"
        android:layout_height="95dp"
        android:layout_marginStart="304dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/button"
        android:text="@string/simulate"
        android:textColor="#000000"
        android:textSize="23sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myRectangleView5" />

    <Button
        android:id="@+id/CalcInsDos"
        android:layout_width="290dp"
        android:layout_height="95dp"
        android:layout_marginStart="304dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button"
        android:text="@string/calculate_insulin_dose"
        android:textColor="#000000"
        android:textSize="23sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/graph"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/disclaimer"
        android:layout_width="598dp"
        android:layout_height="43dp"
        android:layout_marginBottom="88dp"
        android:background="#C7E1FF"
        android:text="@string/disclaimer"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/instruct"
        android:layout_width="272dp"
        android:layout_height="49dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="152dp"
        android:background="#ACDBFF"
        android:text="@string/instructions"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/myRectangleView" />


    <EditText
        android:id="@+id/CBS"
        android:layout_width="130dp"
        android:layout_height="55dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:hint="@string/CBS_Hint"
        android:inputType="numberDecimal"
        android:textColor="#000000"
        android:textSize="23sp"
        app:layout_constraintEnd_toEndOf="@+id/myRectangleView"
        app:layout_constraintTop_toBottomOf="@+id/TBS" />

    <EditText
        android:id="@+id/CV"
        android:layout_width="130dp"
        android:layout_height="55dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:hint="@string/CV_Hint"
        android:inputType="numberDecimal"
        android:textColor="#000000"
        android:textSize="23sp"
        app:layout_constraintEnd_toEndOf="@+id/myRectangleView"
        app:layout_constraintTop_toBottomOf="@+id/CBS" />

    <EditText
        android:id="@+id/DIA"
        android:layout_width="130dp"
        android:layout_height="55dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:hint="@string/DIA_Hint"
        android:inputType="numberDecimal"
        android:textColor="#000000"
        android:textSize="23sp"
        app:layout_constraintEnd_toEndOf="@+id/myRectangleView"
        app:layout_constraintTop_toBottomOf="@+id/ICR" />

    <EditText
        android:id="@+id/output"
        android:layout_width="148dp"
        android:layout_height="90dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="148dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:focusable="false"
        android:inputType="none"
        android:text="@string/calcval_Dummy"
        android:textAlignment="center"
        android:textColor="#FF0000"
        android:textSize="60sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/calcValtext" />

    <EditText
        android:id="@+id/ICR"
        android:layout_width="130dp"
        android:layout_height="55dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:hint="@string/ICR_Hint"
        android:inputType="numberDecimal"
        android:textColor="#000000"
        android:textSize="23sp"
        app:layout_constraintEnd_toEndOf="@+id/myRectangleView"
        app:layout_constraintTop_toBottomOf="@+id/CV" />

    <EditText
        android:id="@+id/TBS"
        android:layout_width="130dp"
        android:layout_height="55dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:hint="@string/TBS_Hint"
        android:inputType="numberDecimal"
        android:textColor="#000000"
        android:textSize="23sp"
        app:layout_constraintEnd_toEndOf="@+id/myRectangleView"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView" />

    <TextView
        android:id="@+id/calcValtext"
        android:layout_width="290dp"
        android:layout_height="49dp"
        android:layout_marginStart="304dp"
        android:layout_marginTop="8dp"
        android:text="@string/suggested_dose"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView5" />

    <TextView
        android:id="@+id/inputHeading"
        android:layout_width="246dp"
        android:layout_height="46dp"
        android:layout_marginStart="8dp"
        android:text="Inputs"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="35sp"
        app:layout_constraintEnd_toEndOf="@+id/myRectangleView"
        app:layout_constraintHorizontal_bias="0.352"
        app:layout_constraintStart_toStartOf="@+id/myRectangleView"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView" />

    <TextView
        android:id="@+id/textUnits"
        android:layout_width="140dp"
        android:layout_height="90dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="64dp"
        android:text="@string/units"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/output"
        app:layout_constraintTop_toBottomOf="@+id/calcValtext"
        app:layout_constraintVertical_bias="0.092" />

    <TextView
        android:id="@+id/insuDose3"
        android:layout_width="40dp"
        android:layout_height="20dp"
        android:layout_marginStart="236dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="59dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:text="@string/calcval_Dummy"
        android:textAlignment="center"
        android:textColor="#FF0000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/myRectangleView5"
        app:layout_constraintTop_toTopOf="@+id/graph"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/insuDose4"
        android:layout_width="42dp"
        android:layout_height="18dp"
        android:layout_marginStart="236dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="59dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:text="@string/calcval_Dummy"
        android:textAlignment="center"
        android:textColor="#FF0000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/myRectangleView5"
        app:layout_constraintTop_toTopOf="@+id/graph"
        app:layout_constraintVertical_bias="0.035" />

    <TextView
        android:id="@+id/insuDose5"
        android:layout_width="40dp"
        android:layout_height="20dp"
        android:layout_marginStart="476dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="59dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:text="@string/calcval_Dummy"
        android:textAlignment="center"
        android:textColor="#FF0000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/graph"
        app:layout_constraintTop_toTopOf="@+id/graph"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/insuDose6"
        android:layout_width="40dp"
        android:layout_height="20dp"
        android:layout_marginStart="480dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="59dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:text="@string/calcval_Dummy"
        android:textAlignment="center"
        android:textColor="#FF0000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/graph"
        app:layout_constraintVertical_bias="0.035" />

    <TextView
        android:id="@+id/insuDose7"
        android:layout_width="40dp"
        android:layout_height="20dp"
        android:layout_marginStart="540dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="59dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:text="@string/calcval_Dummy"
        android:textAlignment="center"
        android:textColor="#FF0000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/graph"
        app:layout_constraintVertical_bias="0.07" />

    <TextView
        android:id="@+id/CBStext"
        android:layout_width="135dp"
        android:layout_height="55dp"
        android:layout_marginTop="8dp"
        android:text="@string/CBS_Text"
        android:textColor="#000000"
        android:textSize="21sp"
        app:layout_constraintEnd_toStartOf="@+id/CBS"
        app:layout_constraintTop_toBottomOf="@+id/TBStext" />

    <TextView
        android:id="@+id/ICRtext"
        android:layout_width="135dp"
        android:layout_height="55dp"
        android:layout_marginTop="12dp"
        android:text="@string/ITR_Text"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/ICR"
        app:layout_constraintTop_toBottomOf="@+id/CVtext" />

    <TextView
        android:id="@+id/CVtext"
        android:layout_width="135dp"
        android:layout_height="55dp"
        android:layout_marginTop="8dp"
        android:text="@string/CV_Text"
        android:textColor="#000000"
        android:textSize="21sp"
        app:layout_constraintEnd_toStartOf="@+id/CV"
        app:layout_constraintTop_toBottomOf="@+id/CBStext" />

    <TextView
        android:id="@+id/DIAtext"
        android:layout_width="135dp"
        android:layout_height="55dp"
        android:layout_marginTop="8dp"
        android:text="@string/DIA_Text"
        android:textColor="#000000"
        android:textSize="21sp"
        app:layout_constraintEnd_toStartOf="@+id/DIA"
        app:layout_constraintTop_toBottomOf="@+id/ICRtext" />

    <TextView
        android:id="@+id/TBStext"
        android:layout_width="135dp"
        android:layout_height="55dp"
        android:layout_marginTop="52dp"
        android:text="@string/TBS_Text"
        android:textColor="#000000"
        android:textSize="21sp"
        app:layout_constraintEnd_toStartOf="@+id/TBS"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView" />

    <TextView
        android:id="@+id/ghLink"
        android:layout_width="592dp"
        android:layout_height="77dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="8dp"
        android:autoLink="web"
        android:text="@string/github"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</android.support.constraint.ConstraintLayout>